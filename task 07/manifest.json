{
    "manifest_version": 3,
    "name": "Weather Extension",
    "version": "1.0",
    "description": "Get weather information in a popup.",
    "permissions": [
      "activeTab",
      "storage"
     
    ],
  
    
    "action": {
      "default_popup": "weather.html",
      "default_icon": {
        "16": "images/162.png",
        "48": "images/48.png",
        "128": "images/128.png" }}
}